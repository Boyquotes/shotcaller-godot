[sub_resource type="Shader" id="26"]
code = "shader_type canvas_item;

uniform vec4 color:source_color;

uniform float angle = 30.;
uniform float width = 1.;
uniform float scale = 1.;
uniform float speed = .5;

uniform sampler2D blade;

uniform float random_seed = 0.;

float deg_to_rad(float deg){
	return (deg * 3.14159265359) / 180.;
}

vec2 rotateUV(vec2 uv, vec2 pivot, float rotation) {
    float sine = sin(rotation);
    float cosine = cos(rotation);

    uv -= pivot;
    uv.x = uv.x * cosine - uv.y * sine;
    uv.y = uv.x * sine + uv.y * cosine;
    uv += pivot;

    return uv;
}

float random(float uv){
	float result = fract(sin(uv * 12783615.43758));
	return result;
}

float simplex_noise(float uv){
	float rand = random(uv);
	
	float i = floor(uv);
	float f = fract(uv);
	
	float noise = mix(random(i), random(i + 1.0), smoothstep(0.,1.,f));
	
	return noise;
}

float map_range(float value, float min1, float max1, float min2, float max2) {
  return min2 + (value - min1) * (max2 - min2) / (max1 - min1);
}

vec3 hsv2rgb(vec3 c)
{
    vec4 K = vec4(1.0, 2.0 / 3.0, 1.0 / 3.0, 3.0);
    vec3 p = abs(fract(c.xxx + K.xyz) * 6.0 - K.www);
    return c.z * mix(K.xxx, clamp(p - K.xxx, 0.0, 1.0), c.y);
}

void fragment(){
	vec2 rotated_uv = rotateUV(UV, vec2(0.), deg_to_rad(angle));
	
	float gray = rotated_uv.x + random_seed;
	float noise = simplex_noise(gray * scale);
	noise = step(noise,width + map_range(sin(speed * (TIME + random_seed)), -1., 1., 0.01, 0.1));

	float blade_mask = texture(blade, UV).a;
	float mask = blade_mask * noise;
	
	COLOR = vec4(hsv2rgb(vec3(map_range(fract(rotated_uv.x  * 4.), 0.,1.,.15,.5), .2, .95)), 1.) * vec4(mask) + texture(TEXTURE, UV) * (1. - mask);
//	COLOR = vec4(hsv2rgb(vec3(rotated_uv.x * .6, .3, 1.)), 1.);
}"

