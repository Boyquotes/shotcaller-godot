[sub_resource type="Shader" id="17"]
code = "shader_type canvas_item;

uniform vec4 color:source_color;

uniform float angle = 1.;
uniform float width = 1.;
uniform float scale = 1.;
uniform float speed = .5;

uniform float random_seed = 0.;

float deg_to_rad(float deg){
	return (deg * 3.14159265359) / 180.;
}

vec2 rotateUV(vec2 uv, vec2 pivot, float rotation) {
    float sine = sin(rotation);
    float cosine = cos(rotation);

    uv -= pivot;
    uv.x = uv.x * cosine - uv.y * sine;
    uv.y = uv.x * sine + uv.y * cosine;
    uv += pivot;

    return uv;
}

float map_range(float value, float min1, float max1, float min2, float max2) {
  return min2 + (value - min1) * (max2 - min2) / (max1 - min1);
}

float random(float uv){
	float result = fract(sin(uv * 12783615.43758));
	return result;
}

float simplex_noise(float uv){
	float rand = random(uv);
	
	float i = floor(uv);
	float f = fract(uv);
	
	float noise = mix(random(i), random(i + 1.0), smoothstep(0.,1.,f));
	
	return noise;
}

void fragment(){
	float gray = rotateUV(UV, vec2(0.), deg_to_rad(angle)).x + random_seed;	
	float noise = simplex_noise(gray * scale);
	
	float mask = step(noise, width + map_range(sin(speed * (TIME + random_seed)), -1., 1., 0.01, 0.1) );
	
	COLOR = vec4(mask) * color;
}"

